<wsdl:definitions
name="PlugfestDemoService"
targetNamespace="http://ochp.eu/1.4/plugfest"
xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
xmlns:xsd="http://www.w3.org/2001/XMLSchema"
xmlns:tns="http://ochp.eu/1.4/plugfest">

	<!-- =================================================================== Types ==================================================================== -->
	<wsdl:types>
		<xsd:schema>
			<xsd:import namespace="http://ochp.eu/1.4/plugfest" schemaLocation="types-plugfest/message-elements.xsd"/>
		</xsd:schema>
	</wsdl:types>


	<!-- =================================================================== Messages ==================================================================== -->

	<!-- Messages for the Exchange of EMSP certificates handling -->
	<wsdl:message name="GetOEMCertificateRequest">
		<wsdl:part element="tns:GetOEMCertificateRequest" name="parameters" />
	</wsdl:message>

	<wsdl:message name="GetOEMCertificateResponse">
		<wsdl:part element="tns:GetOEMCertificateResponse" name="parameters" />
	</wsdl:message>

	<wsdl:message name="GetContractCertificateRequest">
		<wsdl:part element="tns:GetContractCertificateRequest" name="parameters" />
	</wsdl:message>

	<wsdl:message name="GetContractCertificateResponse">
		<wsdl:part element="tns:GetContractCertificateResponse" name="parameters" />
	</wsdl:message>

	<!-- =================================================================== Port Definitions ==================================================================== -->



  <wsdl:portType name="OCHP_1.4_plugfest">
	<wsdl:documentation>
		OCHP Version 1.4 Plugefest Extension
	</wsdl:documentation>

	<wsdl:operation name="GetContractCertificate">
			<wsdl:input message="tns:GetContractCertificateRequest" />
			<wsdl:output message="tns:GetContractCertificateResponse" />
		</wsdl:operation>
		<wsdl:operation name="GetOEMCertificate">
			<wsdl:input message="tns:GetOEMCertificateRequest" />
			<wsdl:output message="tns:GetOEMCertificateResponse" />
		</wsdl:operation>
	</wsdl:portType>

	<!-- =================================================================== Binding Definitions ==================================================================== -->


	<wsdl:binding name="eCHS-OCHP_1.4_plugfest" type="tns:OCHP_1.4_plugfest">
		<soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http" />


		<!-- This is not necessary, cause it just adds a request header  -->
		<wsdl:operation name="GetOEMCertificate">
			<soap:operation soapAction="https://plugfest_demo.eu/GetOEMCertificate" />
			<wsdl:input>
				<soap:body use="literal" />
			</wsdl:input>
			<wsdl:output>
				<soap:body use="literal" />
			</wsdl:output>
		</wsdl:operation>

		<wsdl:operation name="GetContractCertificate">
			<soap:operation soapAction="https://plugfest_demo.eu/GetContractCertificate" />
			<wsdl:input>
				<soap:body use="literal" />
			</wsdl:input>
			<wsdl:output>
				<soap:body use="literal" />
			</wsdl:output>
		</wsdl:operation>

	</wsdl:binding>


	<!-- =================================================================== Service Definition ==================================================================== -->

	<!--the service url could change later on-->
	<wsdl:service name="e-clearing.net">
		<wsdl:port name="echs_P-System" binding="tns:eCHS-OCHP_1.4_plugfest">
			<soap:address location="https://7kravoouwj.execute-api.eu-west-1.amazonaws.com/test/plugfest" />
		</wsdl:port>
	</wsdl:service>

</wsdl:definitions>
