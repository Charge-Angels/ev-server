	"scripts": {
		"start": "npm run start:prod",
		"start:dev": "nodemon -w dist dist/start.js",
		"start:dev:debug": "nodemon -w src --inspect-brk src/start.js",
		"start:dev:prof": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" NODE_ENV=development node --prof src/start.js",
		"start:dev:doctorprof": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" NODE_ENV=development clinic doctor -- node src/start.js",
		"start:dev:flameprof": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" NODE_ENV=development clinic flame -- node src/start.js",
		"start:dev:bubbleprof": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" NODE_ENV=development clinic bubbleprof -- node src/start.js",
		"start:prod": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" NODE_ENV=production node dist/start.js",
		"start:prod:prof": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" NODE_ENV=production node --prof src/start.js",
		"start:prod:doctorprof": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" NODE_ENV=production clinic doctor -- node src/start.js",
		"start:prod:flameprof": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" NODE_ENV=production clinic flame -- node src/start.js",
		"start:prod:bubbleprof": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" NODE_ENV=production clinic bubbleprof -- node src/start.js",
		"build": "npm run build:prod",
		"build:dev": "webpack --watch --env.env=dev",
		"build:prod": "webpack --env.env=prod",
		"clean-install-win": "rmdir node_modules /S /Q && del package-lock.json && npm cache clear --force && npm install",
		"clean-install-lin": "rm -rf ./node_modules && rm -f ./package-lock.json && npm cache clear --force && npm install",
		"lint": "eslint src",
		"lint-fix": "eslint src --fix",
		"test": "mocha test/**/*Test.js --reporter mochawesome --reporter-options reportDir=outputs/mochawesome-report,json=false",
		"test:parallel": "mocha-parallel-tests test/**/*Test.js --reporter mochawesome --reporter-options reportDir=outputs/mochawesome-report,json=false",
		"test:charging-station": "mocha test/**/OCPP*Test.js --reporter mochawesome --reporter-options reportDir=outputs/mochawesome-report,json=false",
		"test:charging-station-specifics": "mocha test/**/OCPPSpecificsTest.js --reporter mochawesome --reporter-options reportDir=outputs/mochawesome-report,json=false",
		"test:tenant": "mocha test/**/TenantTest.js --reporter mochawesome --reporter-options reportDir=outputs/mochawesome-report,json=false",
		"clinic:clean": "clinic clean",
		"start:email": "maildev --web 8888 --web-ip 127.0.0.1 --smtp 70 --verbose --incoming-user evse-mail-user --incoming-pass evse-mail-pwd"
	},
